Pipeline {
    agent any
    tools {
        maven 'M2_HOME'
    }
    stages{
        stage('maven sonarqbe'){
            steps{
                sh 'mvn sonar:sonar'
            }
        }
        stages{
            stage('maven commands'){
                stage{
                    steps{
                        sh 'mvn clean compile test package'
                    }
                }
            }
        }
    }
}