Pipeline {
    agent any
    tools {
        maven 'M2_HOME'
    }
    stages{
        stage('sonarqube scan'){
        steps{
            sh 'mvn sonar:sonar'
        }
    }
    stages{
        stage('maven commands')
        steps{
            sh 'mvn clean compile install test package build'
        }
    }
        
    
